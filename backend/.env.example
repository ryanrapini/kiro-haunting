# Haunted Home Backend Environment Variables
# These are set automatically by CDK in Lambda environment

# DynamoDB Tables
USER_CONFIG_TABLE=HauntedHome-UserConfig
DEVICES_TABLE=HauntedHome-Devices
HAUNTING_SESSIONS_TABLE=HauntedHome-HauntingSessions

# Cognito
USER_POOL_ID=us-east-1_xxxxxxxxx
USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# AWS Region
AWS_REGION=us-east-1

# OpenAI API Key (stored in SSM Parameter Store)
# Access via: aws ssm get-parameter --name "/haunted-home/openai-api-key" --with-decryption
