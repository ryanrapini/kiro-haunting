# Haunted Home Frontend Environment Variables
# Copy this file to .env and fill in your values

# API Gateway endpoint (from CDK output)
VITE_API_ENDPOINT=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/prod

# Cognito User Pool ID (from CDK output)
VITE_USER_POOL_ID=us-east-1_xxxxxxxxx

# Cognito User Pool Client ID (from CDK output)
VITE_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# AWS Region
VITE_AWS_REGION=us-east-1
